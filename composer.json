{
    "name": "websupport/yii-yii2-helpers",
    "description": "Yii 2 helpers backported to use in Yii 1",
    "type": "library",
    "require": {
        "php": ">=5.3.0",
        "yiisoft/yii": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "yii\\helpers\\": "helpers/",
            "tests\\": "tests/"
        }
    }
}
